{
  "name": "project02",
  "version": "1.0.0",
  "description": "Talent sharing platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run build:css",
    "build:css": "postcss public/css/main.css -o public/css/main.compiled.css && postcss public/css/community.css -o public/css/community.compiled.css",
    "watch:css": "postcss public/css/main.css public/css/community.css --dir public/css --watch"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
